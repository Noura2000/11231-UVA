#include <iostream>
#include <stdio.h>
#include<bits/stdc++.h>

using namespace std;

int main()
{
    int n,m,c;
    while(scanf("%d %d %d",&n,&m,&c)==3)
    {
        if(n==0&&m==0&&c==0) return 0;

        if(c==0)
        {
            cout<<(n-7)*(m-7)/2<<endl;

        }else{
        cout<<((n-7)*(m-7)+1)/2<<endl;
        }

    }


    return 0;
}
